{
    "name": "astar-token-api",
    "version": "1.0.1",
    "description": "Astar Network token API",
    "private": true,
    "keywords": [
        "astar-network",
        "typescript",
        "api"
    ],
    "homepage": "https://github.com/AstarNetwork/astar-token-api",
    "bugs": {
        "url": "https://github.com/AstarNetwork/astar-token-api.git/issues"
    },
    "author": {
        "name": "Bobo K.",
        "url": "https://github.com/bobo-k2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/AstarNetwork/astar-token-api.git"
    },
    "main": "build/index.js",
    "scripts": {
        "serve:firebase": "yarn build && firebase emulators:start --only functions",
        "shell:firebase": "yarn build && firebase functions:shell",
        "build": "tsc --project tsconfig.json",
        "dev": "yarn swagger-autogen && ts-node-dev -r dotenv/config src/index.ts",
        "build:swagger": "yarn swagger-autogen api.astar.network",
        "lint": "eslint '*/**/*.{js,ts}' --quiet --fix && prettier -w .",
        "lint:check": "eslint '*/**/*.{js,ts}' && prettier -c .",
        "test": "jest",
        "swagger-autogen": "node src/swagger.js",
        "deploy": "firebase deploy"
    },
    "engines": {
        "node": "16"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.16.12",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-typescript": "^7.16.7",
        "@types/cors": "^2.8.12",
        "@types/dedent": "^0.7.0",
        "@types/express": "^4.17.13",
        "@types/ioredis": "^4.28.1",
        "@types/jest": "^27.4.0",
        "@types/luxon": "^2.0.7",
        "@types/node": "^16.9.3",
        "@types/node-fetch": "2.5.10",
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "babel-jest": "^27.4.6",
        "dotenv": "^10.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.4.7",
        "jest-config": "^27.2.0",
        "nock": "^13.1.3",
        "prettier": "^2.4.1",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.2.1",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.4.3"
    },
    "dependencies": {
        "@babel/plugin-proposal-decorators": "^7.16.7",
        "@discordjs/rest": "^0.1.0-canary.0",
        "@polkadot/api": "^8.9.1",
        "@polkadot/keyring": "^9.6.1",
        "@polkadot/util": "^9.6.1",
        "@polkadot/util-crypto": "^9.6.1",
        "abi-decoder": "^2.4.0",
        "apr-tools": "^0.1.3",
        "axios": "^0.24.0",
        "body-parser": "^1.20.0",
        "cors": "^2.8.5",
        "dedent": "^0.7.0",
        "discord-api-types": "^0.23.1",
        "discord.js": "^13.1.0",
        "ethers": "^5.5.4",
        "express": "^4.17.1",
        "express-validator": "^6.14.1",
        "firebase-admin": "^10.2.0",
        "firebase-functions": "^3.21.2",
        "inversify": "^6.0.1",
        "ioredis": "^4.28.0",
        "luxon": "^2.1.1",
        "reflect-metadata": "^0.1.13",
        "swagger-autogen": "^2.19.0",
        "web3": "^1.7.0"
    }
}
